{
    "name": "img/image-bundle",
    "type": "symfony-bundle",
    "description": "Image upload, crop and gallery bundle for Symfony 3",
    "keywords": ["Form Type", "image", "upload", "crop", "jcrop", "jquery upload"],
    "homepage": "https://github.com/ImaginaLabs/IMGImageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Cotonnec",
            "email": "steve.cotonnec@imagina.io",
            "homepage": "http://imagina.io"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-gd": "*",
        "symfony/http-foundation": "~2.2",
        "symfony/form": "~2.2|3.*|4.*",
        "symfony/validator": "~2.2|3.*|4.*",
        "symfony/routing": "~2.2|3.*|4.*",
        "symfony/config": "~2.2",
        "symfony/twig-bridge": "~2.2|3.*|4.*",
        "symfony/class-loader":"~2.2",
        "twig/twig": "~1.12|4.*",
        "twig/extensions": "~1.0",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "jms/translation-bundle": "@stable"
    },
    "target-dir": "ImaginaLabs/IMGImageBundle",
    "autoload": {
        "psr-0": { "ImaginaLabs\\IMGImageBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
